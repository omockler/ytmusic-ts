{
  "name": "ytmusic-ts",
  "version": "0.2.0",
  "type": "module",
  "sideEffects": false,
  "description": "TypeScript port of ytmusicapi - interact with YouTube Music API",
  "license": "MIT",
  "author": "omockler",
  "repository": {
    "type": "git",
    "url": "https://github.com/omockler/ytmusic-ts.git"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./auth": {
      "import": {
        "types": "./dist/auth/index.d.ts",
        "default": "./dist/auth/index.js"
      },
      "require": {
        "types": "./dist/auth/index.d.cts",
        "default": "./dist/auth/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "ci": "yarn typecheck && yarn lint && yarn test && yarn build",
    "prepublishOnly": "yarn ci"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
